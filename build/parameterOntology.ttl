@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dhpluso: <https://dh.plus.ac.at/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix parameter: <https://dh.plus.ac.at/parameter-ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://dh.plus.ac.at/parameter-ontology> a owl:Ontology ;
    rdfs:label "dhPLUS SHACL Parameter Ontologie"@de,
        "dhPLUS SHACL Parameter Ontology"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dc:rights <https://creativecommons.org/publicdomain/zero/1.0/> ;
    dc:title "dhPLUS SHACL Parameter Ontologie"@de,
        "dhPLUS SHACL Parameter Ontology"@en ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    owl:ontologyIRI parameter: ;
    owl:versionIRI <https://dh.plus.ac.at/parameter-ontology/0-1#> ;
    owl:versionInfo "2021-05-12"^^xsd:date,
        "0.1"^^xsd:string ;
    skos:definition "Dies ist eine Ontologie, die verwendet wird, um Parametersätze zu spezifizieren, die vom shacl-Infer-Prozess in dhPLUS verwendet werden."@de,
        "This is an ontology used to specify parameter sets used by shacl infer process in dhPLUS."@en .

parameter:InputParameterSet a owl:Class ;
    rdfs:label "Parameterset Input"@de,
        "Input parameter set"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-07-20" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:subClassOf parameter:ParameterSet ;
    skos:definition "Ein Satz von Parametern, der als Input fungiert."@de,
        "A set of parameters to be used as input."@en .

parameter:OutputParameterSet a owl:Class ;
    rdfs:label "Parameterset Output"@de,
        "Output parameter set"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:subClassOf parameter:ParameterSet ;
    skos:definition "Ein Satz von Parametern generiert zum nächsten Input."@de,
        "A set of parameters generated by shacl processor to be used a next input."@en .

parameter:addAdditionalTriples a owl:ObjectProperty ;
    rdfs:label "füge weitere Triple hinzu"@de,
        "add additional Triples"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:domain parameter:ParameterSet ;
    rdfs:range parameter:Statement ;
    skos:definition "Zeigt auf die Statements eines Parametersatzes."@de,
        "Points to the statements of a parameter set."@en .

parameter:addReificationStatements a owl:DatatypeProperty ;
    rdfs:label "Reifizierungsanweisungen hinzufügen"@de,
        "Add Reification Statements"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-10-12+07:47" ;
    rdfs:domain parameter:ParameterSet ;
    rdfs:range xsd:boolean ;
    skos:definition "Wenn gesetzt, werden Reification-Statements generiert, um die Quelle zu jedem Triple zu dokumentieren."@de,
        "If set, reification statements are generated to record the source of each triple."@en .

parameter:authorityUri a owl:ObjectProperty ;
    rdfs:label "Authority Uri"@de,
        "Authority Uri"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:domain parameter:InstanceParameter ;
    rdfs:range xsd:anyURI ;
    skos:definition "Uri eines Normdatendatzes."@de,
        "Uri of an authority file."@en .

parameter:dhpClass a owl:ObjectProperty ;
    rdfs:label "dhplus Ontologie Klasse"@de,
        "dhplus ontology class"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:domain parameter:InstanceParameter ;
    rdfs:range dhpluso:Entity ;
    skos:definition "Uri einer dhplus Ontologie Klasse."@de,
        "Uri of a dhplus ontology class."@en .

parameter:dhpUri a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "dhplus Uri"@de,
        "dhplus uri"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:domain parameter:InstanceParameter ;
    rdfs:range xsd:anyURI ;
    skos:definition "Uri der dhplus-Instanz, die generiert werden soll."@de,
        "Uri of the dhplus instance to infer."@en .

parameter:expandViaWikiData a owl:DatatypeProperty ;
    rdfs:label "erweitere über Wikidata"@de,
        "expand via Wikidata"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:domain parameter:ParameterSet ;
    rdfs:range xsd:boolean ;
    skos:definition "Wenn gesetzt, wird nach weiteren Normdaten über Wikidata gesucht und diese werden ebenfalls bei der Generierung berücksichtigt."@de,
        "If set, further norm data is searched for via Wikidata and these are also taken into account during generation."@en .

parameter:filterLanguage a owl:DatatypeProperty ;
    rdfs:label "Sprachfilter"@de,
        "filter Language"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:domain parameter:ParameterSet ;
    rdfs:range xsd:string ;
    skos:definition "Wenn gesetzt, werden nur Literale der passenden Sprache generiert. Beispiel: 'en,de'"@de,
        "If set, only literals of the matching language will be infered. Example: 'en,de'"@en .

parameter:hasParameter a owl:ObjectProperty ;
    rdfs:label "hat Parameter"@de,
        "has parameter"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:domain parameter:ParameterSet ;
    rdfs:range parameter:Parameter ;
    skos:definition "Zeigt auf die Parameter eines Parametersatzes."@de,
        "Points to the parameters of a parameter set."@en .

parameter:object a owl:ObjectProperty ;
    rdfs:label "object"@de,
        "object"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:domain parameter:Statement ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf rdf:object ;
    skos:definition "object eines statement."@de,
        "object of a statement."@en .

parameter:predicate a owl:ObjectProperty ;
    rdfs:label "predicate"@de,
        "predicate"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:domain parameter:Statement ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf rdf:predicate ;
    skos:definition "predicate eines statement."@de,
        "predicate of a statement."@en .

parameter:subject a owl:ObjectProperty ;
    rdfs:label "subject"@de,
        "subject"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:domain parameter:Statement ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf rdf:subject ;
    skos:definition "subject eines statement."@de,
        "subject of a statement."@en .

parameter:Parameter a owl:Class ;
    rdfs:label "Parameter"@de,
        "Parameter"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    skos:definition "Klasse eines Parameters."@de,
        "Class of a parameter."@en .

parameter:InstanceParameter a owl:Class ;
    rdfs:label "Instanz Parameter"@de,
        "Instance parameter"@en ;
    rdfs:subClassOf parameter:Parameter ;
    skos:definition "Klasse eines Instanzparameters, die verwendet wird, um den Generiegungsprozess einer Instanz zu definieren."@de,
        "Class of an instance parameter, used to define the infer process of one instance."@en .

parameter:Statement a owl:Class ;
    rdfs:label "Statement"@de,
        "Statement"@en ;
    dc:creator "Peter Hinkelmanns" ;
    dcterms:created "2021-05-12" ;
    dcterms:modified "2021-09-16+13:18" ;
    rdfs:subClassOf rdf:Statement ;
    skos:definition "Klasse eines Statements."@de,
        "Class of a statement."@en .

parameter:ParameterSet a owl:Class ;
    rdfs:label "Parameterset"@de,
        "Parameter set"@en ;
    skos:definition "Ein Satz von Parametern."@de,
        "A set of parameters."@en .

